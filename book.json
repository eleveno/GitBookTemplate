{
    "title": "GitBook使用教程",
    "author": "Guojie",
    "description": "select * from learn",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "structure": {
        "readme": "README.md"
    },
    "links": {
        "sidebar": {
            "小羊窝": "https://eleveno.github.io/"
        }
    },
    "plugins": [
        "-sharing",
        "splitter",
        "expandable-chapters-small",
        "anchors",
        "github",
        "github-buttons",
        "donate",
        "sharing-plus",
        "anchor-navigation-ex",
        "favicon"
    ],
    "pluginsConfig": {
        "github": {
            "url": "https://eleveno.github.io/"
        },
        "github-buttons": {
            "buttons": [
                {
                    "user": "Eleveno",
                    "repo": "glory",
                    "type": "star",
                    "size": "small",
                    "count": true
                }
            ]
        },
        "donate": {
            "alipay": "/source/images/alipay_collect_qrcode.JPG",
            "title": "救救孩子吧",
            "button": "赞赏",
            "alipayText": "可否赏小爷几个包子"
        },
        "sharing": {
            "douban": false,
            "facebook": false,
            "google": false,
            "hatenaBookmark": false,
            "instapaper": false,
            "line": false,
            "linkedin": false,
            "messenger": false,
            "pocket": false,
            "qq": true,
            "qzone": false,
            "stumbleupon": false,
            "twitter": false,
            "viber": false,
            "vk": false,
            "weibo": true,
            "whatsapp": false,
            "all": [
                "google",
                "facebook",
                "weibo",
                "twitter",
                "qq",
                "qzone",
                "linkedin",
                "pocket"
            ]
        },
        "anchor-navigation-ex": {
            "showLevel": false
        },
        "favicon": {
            "shortcut": "/source/images/test.JPG",
            "bookmark": "/source/images/test.JPG",
            "appleTouch": "/source/images/test.JPG",
            "appleTouchMore": {
                "120x120": "/source/images/test.JPG",
                "180x180": "/source/images/test.JPG"
            }
        }
    }
}